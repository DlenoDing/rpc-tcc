{
    "name": "dleno/rpc-tcc",
    "type": "library",
    "description": "rpc tcc transaction(PHP)",
    "keywords": ["rpc", "tcc", "transaction"],
    "authors": [
        {
            "name": "dleno",
            "email": "dleno@126.com",
            "homepage": "https://www.dlenos.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-swoole": ">=4.7",
        "dleno/common-core": "~2.2.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Dleno\\RpcTcc\\": "src/"
        },
        "files": []
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "Dleno\\RpcTcc\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
